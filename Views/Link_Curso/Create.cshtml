@model NewJob.Models.Link_Curso

@{
    ViewData["Title"] = "Create Link_Curso";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Criar</title>
</head>
<body>

<h4>Cursos</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Link_Cursos" class="control-label"></label>
                <input asp-for="Link_Cursos" class="form-control" />
                <span asp-validation-for="Link_Cursos" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Adicionar link" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Ir para a lista de Cadastro</a>
</div>

</body>
</html>
